    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: guicalculator with Arguments",
            "type": "debugpy",
            "request": "launch",
            "module": "guicalculator",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Python Debugger: guicalculator",
            "type": "debugpy",
            "request": "launch",
            "module": "guicalculator",
        },
        {
            "name": "Python Debugger: guicalculator (logging=warn)",
            "type": "debugpy",
            "request": "launch",
            "module": "guicalculator",
            "args": [
                "-l",
                "warn"
            ]
        },
    ]
}